		"iis": {
			"applicationUrl": "https://localhost/Skoruba.IdentityServer4.Admin",
			"sslPort": 0
		},
		"iisExpress": {
			"applicationUrl": "http://localhost:9000/",
			"sslPort": 0
		}
	},
	"profiles": {
		"IIS Express": {
			"commandName": "IISExpress",
			"commandLineArgs": "/seed",
